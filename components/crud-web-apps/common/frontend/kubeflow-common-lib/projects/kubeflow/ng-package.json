{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/kubeflow",
  "assets": [
    {
      "input": "src/assets",
      "glob": "**/*",
      "output": "assets"
    },
    {
      "input": "src/styles",
      "glob": "**/*",
      "output": "styles"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  }
}
